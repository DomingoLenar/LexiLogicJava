<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="changeUsernamePane" prefHeight="303.0" prefWidth="464.0" style="-fx-background-color: #674C33;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.amalgam.client.profile.ProfileChangeUsernameController">
   <children>
      <ImageView fx:id="axelImage" fitHeight="184.0" fitWidth="181.0" layoutX="-18.0" layoutY="119.0" pickOnBounds="true">
         <image>
            <Image url="@../../../icons/Axel.png" />
         </image>
      </ImageView>
      <ImageView fx:id="creamRectangleImage" fitHeight="309.0" fitWidth="325.0" layoutX="125.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../icons/CreamRectangle.png" />
         </image>
      </ImageView>
      <Label fx:id="usernameLabel" layoutX="212.0" layoutY="59.0" text="USERNAME" textFill="#6b4406">
         <font>
            <Font name="Bowlby One SC Regular" size="24.0" />
         </font>
      </Label>
      <Label fx:id="scoreLabel" layoutX="213.0" layoutY="87.0" text="20238" textFill="#485613">
         <font>
            <Font name="Bowlby One SC Regular" size="20.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="25.0" layoutY="22.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="25.0" style="-fx-background-color: #FFFFFFF;" />
      <ImageView fx:id="backButtonImage" fitHeight="60.0" fitWidth="71.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../icons/BackButton.png" />
         </image>
      </ImageView>
      <ImageView fx:id="profileImage" fitHeight="60.0" fitWidth="71.0" layoutX="141.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../icons/Profile.png" />
         </image>
      </ImageView>
      <TextField fx:id="newUsernameField" layoutX="176.0" layoutY="152.0" prefHeight="40.0" prefWidth="224.0" promptText="new username" style="-fx-background-color: #B07C3B; -fx-prompt-text-fill: #FFFFFF;">
         <font>
            <Font name="Brygada 1918 Regular" size="18.0" />
         </font>
      </TextField>
      <Button fx:id="saveButton" layoutX="332.0" layoutY="211.0" mnemonicParsing="false" style="-fx-background-color: #9CA16F;" text="SAVE" textFill="WHITE">
         <font>
            <Font name="Brygada 1918 Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
